<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisrealation.mapper.SysLogMapper">

    <select id="insertLog" parameterType="com.example.mybatisrealation.bean.SysLog" resultType="java.lang.Integer">
        insert into sys_log (id,user_id,user_action,create_time) values (#{id},#{userId},#{userAction},#{createTime})
    </select>
</mapper>